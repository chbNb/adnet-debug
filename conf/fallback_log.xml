<seelog>
    <outputs formatid="watch">
        <filter levels="info">
            <buffered formatid="watch" size="10000" flushperiod="1000">
                <rollingfile type="date" filename="/data/adn_logs/adnet/fallback/fallback.log" datepattern="2006-01-02-15" maxrolls="240"/>
            </buffered>
        </filter>
    </outputs>
    <formats>
        <format id="watch" format="%Date %Time%Msg%n"/>
    </formats>
</seelog>